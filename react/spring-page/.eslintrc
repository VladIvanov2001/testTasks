{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "quotes": [2, "single", { "avoidEscape": true }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
    ]
  }
}
